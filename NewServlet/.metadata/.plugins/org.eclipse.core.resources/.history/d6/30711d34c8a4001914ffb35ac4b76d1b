package companyData;
import java.sql.*;
public class corporate {
	public static void main(String[] args) 
	throws SQLException,ClassNotFoundException {
		Class.forName("oracle.jdbc.driver.OracleDriver");
		Connection con=DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:orcl", "scott", "1234");
		Statement stm=con.createStatement();
		ResultSet rs=stm.executeQuery("select * from company");
		while(rs.next()) {
			System.out.println(rs.getInt(1)+"\t"+rs.getString(2)+"\t"+rs.getLong(3)+"\n"+rs.getString(4)
			+"\t"+rs.getString(5));
		}
		con.close();
	}
}
