package SCode;
import java.util.*;
public class BubbleSortTest {
	static int[] a;
	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		System.out.println("enter the number : ");
		int n=scn.nextInt();
		
		Random r=new Random();
		
		System.out.println();
		for(int i=0;i<n;i++) {
			int size=r.nextInt(n);
			a[i]=size;
		}
		System.out.println(a);
	}

}
