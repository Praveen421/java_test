package logical;
import java.util.*;
public class HackerRank3 {

	public static void main(String[] args) {
		double a=0,b=2,n=10;
		double sum=0;
		for(int i=0;i<n;i++) {
			sum=a+b*Math.pow(2, i); 
			System.out.print(sum+" ");
		}
		

		/*double a=2;
