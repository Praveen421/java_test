package SCode;
import java.util.*;
public class CompanyTest {
	public static void main(String[] args) {
		Random r=new Random();		
		for(int i=0;i<10;i++) {
		int a=10+r.nextInt(100000);
		System.out.print(a+"\t\t");
		int count=0;
		while(a!=0) {
			a=a/10;
			count++;
		}
		System.out.println(count);
		}
	}
}
