package SCode;
import java.util.*;
public class StringTest04 {
	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		System.out.println("enter the String : ");
		String s=scn.nextLine();
		s.replaceAll("A", "a");
		s.replaceAll("E", "e");
		s.replaceAll("I", "i");
		s.replaceAll("O", "o");
		s.replaceAll("U", "u");
		System.out.println(s);
	}
}
