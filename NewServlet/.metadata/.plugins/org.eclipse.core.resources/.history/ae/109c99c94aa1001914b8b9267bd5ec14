package SCode;
import java.util.*;
public class PatternTest {
	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		System.out.println("enter the no . ");
		int n=scn.nextInt();
		for(int i=n;i>0;i--) {
			for(int j=0;j<n;j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
