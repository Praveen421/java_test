package SCode;
import java.util.*;
public class PatternTest {
	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		System.out.println("enter the no . ");
		int n=scn.nextInt();
		for(int i=0;i<n;i++) {
			for(int j=n;j>0;j--) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
