package SCode;
import java.util.*;
public class StringTest04 {
	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		System.out.println("enter the String : ");
		String s=scn.nextLine();
		s.replaceAll("a", "A");
		s.replaceAll("e", "E");
		s.replaceAll("i", "I");
		s.replaceAll("o", "O");
		s.replaceAll("u", "U");
		System.out.println(s);
	}
}
