package test02;
public class intern_Test {
	public static void main(String[] args) {
		String s1="Hari";
		String s2=s1.intern();
		System.out.println(s1);
		System.out.println(s2);
		System.out.println(s1==s2);
		
		String s3=new String("Hari");
		String s4=s3.intern();
		System.out.println(s3);
		System.out.println(s4);
	}
}
