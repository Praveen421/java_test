package stringhandling;

public class CharAtTest {

	public static void main(String[] args) {
		String s1="abcdef";
		System.out.println("s1: "+s1);
		System.out.println("s1: "+s1.charAt(0));
		System.out.println("s1: "+s1.charAt(1));
		System.out.println("s1: "+s1.charAt(2));
		System.out.println("s1: "+s1.charAt(3));
		System.out.println("s1: "+s1.charAt(4));
		System.out.println("s1: "+s1.charAt(5));
		System.out.println("s1: "+s1.charAt(6));

	}

}
